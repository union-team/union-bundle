{
    "name": "union-team/union-bundle",
    "description": "Useful functionality for a union team project.",
    "minimum-stability": "stable",
    "license": "proprietary",
    "type": "symfony-bundle",
    "prefer-stable": true,
    "keywords": [
        "bundle",
        "union"
    ],
    "authors": [
        {
            "name": "abdelhak",
            "email": "abdelhak.freelance@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/framework-bundle": "^7.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/test-pack": "*"
    },
    "autoload": {
        "exclude-from-classmap": [
            "Tests/"
        ],
        "psr-4": {
            "Union\\Bundle\\UnionBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Union\\Bundle\\UnionBundle\\Tests\\": "Tests"
        }
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "8.2"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    }
}
